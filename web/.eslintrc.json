{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@next/next/no-html-link-for-pages": "off",
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "quote-props": "off",
    "space-before-function-paren": [
      "error", {
        "anonymous": "always",
        "named": "ignore",
        "asyncArrow": "always"
      }
    ]
  }
}
